{
  "name": "amc-8-problem-generator",
  "version": "0.0.0",
  "description": "AMC 8 Problem Generator",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "minify:problems": "uglifyjs --compress --mangle --comments --comments '/(^!|@license|@preserve)/' -o dist/problems.js -- problems.js",
